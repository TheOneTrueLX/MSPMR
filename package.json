{
  "name": "mspmr",
  "version": "1.0.0",
  "description": "Media Share Manager for Live Streamers",
  "repository": "git@github.com:TheOneTrueLX/MSPMR.git",
  "author": "TheOneTrueLX <lx@opx.me>",
  "license": "Unlicense",
  "private": true,
  "scripts": {
    "frontend:dev": "cd frontend && vite",
    "frontend:build": "cd frontend && vue-tsc --noEmit && vite build",
    "frontend:preview": "cd frontend && vite preview",
    "api:start": "cd services/api && node dist/index.js",
    "api:compile": "cd services/api && ts-node build.ts && tsc",
    "api:dev": "cd services/api && nodemon server/index.ts | pino-pretty",
    "api:dev:debug": "cd services/api && nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "api:test": "cd services/api && mocha test/**/*.ts --exit",
    "api:test:debug": "cd services/api && mocha --inspect-brk test/**/*.ts --exit",
    "twitchlistener:dev": "cd services/twitch-listener && nodemon src/index.ts | pino-pretty"
  },
  "dependencies": {
    "@twurple/api": "^5.0.18",
    "@twurple/auth": "^5.0.18",
    "@twurple/chat": "^5.0.18",
    "@twurple/eventsub": "^5.0.18",
    "@twurple/eventsub-ngrok": "^5.0.18",
    "@twurple/pubsub": "^5.0.18",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "google-auth-library": "^7.14.0",
    "googleapis": "^96.0.0",
    "http-status-codes": "^1.3.0",
    "mongoose": "^5.3.11",
    "node-fetch": "^3.2.2",
    "pino": "^5.8.1",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "swagger-express-middleware": "^1.1.1",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.3.1",
    "@types/pino": "^5.20.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "chai": "^4.1.2",
    "esbuild-wasm": "^0.14.25",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.6",
    "pino-pretty": "^2.2.3",
    "postcss": "^8.4.8",
    "shelljs": "^0.8.3",
    "supertest": "^3.3.0",
    "tailwindcss": "^3.0.23",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  }
}
