    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "google-auth-library": "^7.14.0",
    "googleapis": "^96.0.0",
    "http-status-codes": "^1.3.0",
    "mongoose": "^5.3.11",
    "node-fetch": "^3.2.2",
    "node-redis": "^0.1.7",
    "pino": "^5.8.1",
    "retry": "^0.13.1",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "swagger-express-middleware": "^1.1.1",
