{
  "name": "mspmr",
  "version": "1.0.0",
  "private": true,
  "description": "Media Share Manager for Live Streamers",
  "author": "TheOneTrueLX <lx@opx.me>",
  "scripts": {
    "api:compile": "cd services/api && ts-node build.ts && tsc",
    "api:dev": "cd services/api && nodemon server/index.ts | pino-pretty",
    "api:dev:debug": "cd services/api && nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "api:start": "cd services/api && node dist/index.js",
    "api:test": "cd services/api && mocha test/**/*.ts --exit",
    "api:test:debug": "cd services/api && mocha --inspect-brk test/**/*.ts --exit",
    "frontend:serve": "cd frontend && vue-cli-service serve",
    "frontend:build": "cd frontend && vue-cli-service build",
    "frontend:lint": "cd frontend && vue-cli-service lint",
    "twitchlistener:dev": "cd services/twitch-listener && nodemon src/index.ts | pino-pretty"
  },
  "dependencies": {
    "@node-redis/client": "^1.0.4",
    "@twurple/api": "^5.0.18",
